{
  "name": "purrgrammer",
  "displayName": "Purrgrammer: Kitty Pomodoro Timer",
  "description": "A little companion to keep you focused and productive while coding.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "publisher": "ndrada",
  "keywords": [
    "cat",
    "kitty",
    "pomodoro",
    "timer",
    "focus",
    "productivity",
    "coding",
    "work",
    "study",
    "relax",
    "pet",
    "pixelart",
    "gamified",
    "breaks",
    "reminders"
  ],
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "purrgrammerView",
          "title": "Purrgrammer",
          "icon": "media/cat-icon.svg"

        }
      ]
    },
    "views": {
      "purrgrammerView": [
        {
          "id": "purrgrammerPanel",
          "name": "Kitty Companion",
          "type": "webview"

        }
      ]
    },
    "commands": [{
      "command": "purrgrammer.helloWorld",
      "title": "Hello World"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
